{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "buildCommand": "yarn build:all",
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@2.0.0",
      "memory": 1024,
      "maxDuration": 10
    },
    "packages/twenty-server/api/*.ts": {
      "runtime": "@vercel/node@2.0.0",
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "regions": [
    "iad1"
  ],
  "cleanUrls": true
}