# --- Supabase Database ---
DATABASE_URL=postgres://username:password@host:port/dbname
# or use POSTGRES_URL if that's what your code expects
POSTGRES_URL=postgres://username:password@host:port/dbname

# --- Supabase Keys ---
SUPABASE_URL=https://your-supabase-project.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# --- Redis Upstash ---
REDIS_URL=rediss://<username>:<password>@<host>:<port>

# --- JWT / Secrets ---
JWT_SECRET=your-jwt-secret
# Add any other keys required by your backend

# --- Vercel Environment ---
IS_SERVERLESS=true
SQLITE_DISABLED=true

# --- Other Vercel/DB Vars (optional, if used in code) ---
PG_DATABASE_URL=${POSTGRES_URL}
POSTGRES_HOST=your-db-host
POSTGRES_USER=your-db-user
POSTGRES_PASSWORD=your-db-password
POSTGRES_DATABASE=your-db-name

# Add any other required environment variables below
